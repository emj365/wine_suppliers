{
  "name": "Region",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": true,
  "replaceOnPUT": true,
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "country": {
      "type": "belongsTo",
      "model": "Country",
      "foreignKey": "countryId"
    },
    "winery": {
      "type": "hasMany",
      "model": "Winery",
      "foreignKey": "regionId",
      "description": "the region has many winery"
    }
  },
  "acls": [],
  "methods": {}
}
